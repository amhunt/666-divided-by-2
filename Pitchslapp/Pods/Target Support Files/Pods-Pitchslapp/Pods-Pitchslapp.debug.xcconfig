EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$(PODS_ROOT)/Firebase"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AudioPlayer.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Firebase.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FirebaseUI.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftForms.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AudioPlayer" -framework "Firebase" -framework "FirebaseUI" -framework "SwiftForms"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Pitchslapp
PODS_ROOT = ${SRCROOT}/Pods